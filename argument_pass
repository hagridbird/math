package com.math;

public class Math {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a =0;
		String S = null;
		try {
			a = Integer.parseInt(args[0]);
			S = args[1];
		} catch (NumberFormatException e) {
			// TODO Auto-generated catch block
			//e.printStackTrace();
		}
		finally
		{
		System.out.println(a);
		System.out.println(S);
		}
	}

}
